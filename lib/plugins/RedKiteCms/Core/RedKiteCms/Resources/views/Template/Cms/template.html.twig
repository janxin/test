{% extends 'RedKiteCms/Resources/views/Template/base.html.twig' %}

{% block external_stylesheets %}
    {% for value in template_assets_manager.getExternalStylesheets(['cssrewrite']) %}
    <link href="{{ value }}" rel="stylesheet" type="text/css" media="all" />
    {% endfor %}
{% endblock %}

{% block external_javascripts %}
    {% for value in template_assets_manager.getExternalJavascripts %}
    <script src="{{ value }}"></script>
    {% endfor %}
    <script src="/components/tinymce/tinymce.min.js"></script>    
    <script src="/components/ace/src-min-noconflict/ace.js"></script>
{% endblock %}